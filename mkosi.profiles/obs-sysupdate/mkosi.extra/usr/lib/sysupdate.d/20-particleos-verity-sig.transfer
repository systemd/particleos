[Transfer]
ProtectVersion=%A

[Source]
Type=url-file
Path=https://download.opensuse.org/repositories/system:/systemd/%o_%w_images/
MatchPattern=ParticleOS_@v_%a.usr-%a-verity-sig.@u.raw.zst

[Target]
Type=partition
Path=auto
MatchPattern=ParticleOS_@v_verity_sig
MatchPartitionType=usr-verity-sig
PartitionFlags=0
ReadOnly=1
