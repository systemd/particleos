[Transfer]
ProtectVersion=%A

[Source]
Type=url-file
Path=https://download.opensuse.org/repositories/home:/bluca:/branches:/home:/bluca:/systemd/Debian_Testing_images/
MatchPattern=ParticleOS_@v_%a.usr-%a-verity.@u.raw.zst

[Target]
Type=partition
Path=auto
MatchPattern=ParticleOS_@v_verity
MatchPartitionType=usr-verity
PartitionFlags=0
ReadOnly=1
